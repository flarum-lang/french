    "type": "flarum-extension",
    "keywords": ["extension", "flarum", "french", "locale", "translation"],
    "license": "MIT",
    "authors": [
        {
            "name": "Qiaeru",
            "email": "qiaeru@protonmail.com",
            "homepage": "https://www.qiaeru.com"
        }
    ],
    "support": {
        "issues": "https://github.com/qiaeru/lang-french/issues",
        "source": "https://github.com/qiaeru/lang-french"
